public class ZweiBeiner extends Tier {
    public ZweiBeiner(int alter, String name) {
        super(alter, name);
    }

    public ZweiBeiner() {}

    @Override
    public void bewegen(){
        System.out.println("Zwei Beine bewegen");
    }

    @Override
    public void fressen(){
        System.out.println("Nom Nom, ich habe zwei Beine")
    }


}